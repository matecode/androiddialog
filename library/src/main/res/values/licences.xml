    <string name="mit_copyright">Copyright (c) %1$d %2$s</string>
    <string name="agpl30_copyright">Copyright (c) %1$d %2$s</string>
    <style name="LicenceTitle" parent="Base.TextAppearance.AppCompat.Medium">
        <item name="android:textStyle">bold</item>
        <item name="android:paddingTop" type="dimen">8dp</item>
        <item name="android:paddingBottom" type="dimen">8dp</item>
